{
	"name": "npc-editor",
	"description": "editor for npc",
	"version": "0.0.2",
	"scripts": {
		"execute": "cross-env NODE_ENV=dev node node_modules/electron/cli.js .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder"
	},
	"main": "main.js",
	"build": {
		"appId": "npc.editor",
		"extraResources": [
			"NPC.Runtime.exe"
		]
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"css-loader": "^6.5.1",
		"electron": "^15.3.1",
		"electron-builder": "^22.13.1",
		"file-loader": "^6.2.0",
		"glob": "^7.2.0",
		"html-webpack-plugin": "^5.5.0",
		"monaco-editor": "^0.30.1",
		"monaco-editor-webpack-plugin": "^6.0.0",
		"style-loader": "^3.3.1",
		"terser-webpack-plugin": "^5.2.5",
		"ts-loader": "^9.2.6",
		"typescript": "^4.4.4",
		"webpack": "^5.64.1",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.5.0",
		"yaserver": "^0.4.0"
	},
	"dependencies": {
		"electron-log": "^4.4.1"
	}
}
